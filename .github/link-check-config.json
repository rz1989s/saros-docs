{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://api\\."
    },
    {
      "pattern": "YOUR_.*"
    },
    {
      "pattern": "EXAMPLE_.*"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206],
  "httpHeaders": [
    {
      "urls": ["https://github.com/"],
      "headers": {
        "Accept": "text/html",
        "User-Agent": "Mozilla/5.0 (X11; Ubuntu; Linux x86_64; rv:95.0) Gecko/20100101 Firefox/95.0"
      }
    }
  ]
}